{
  "name": "storelab",
  "version": "1.0.0",
  "description": "storelab",
  "license": "ISC",
  "author": "dinhvaren",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "formatter": "prettier 'src/**/*.{js,scss,json,hbs}' --tab-width 4 --write --single-quote --trailing-comma all",
    "start": "nodemon --inspect src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "sass --watch src/resources/scss:src/public/css",
    "build:scss": "sass --watch src/css/app.scss:src/css/app.css",
    "build:css": "npx tailwindcss -i ./src/css/app.css -o ./public/css/app.css --watch",
    "dev": "npm run build:scss & npm run build:css"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "mongoose": "^8.18.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "sass": "^1.93.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.2",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "mongoose-delete": "^1.0.2",
    "mongoose-sequence": "^6.0.1",
    "mongoose-slug-generator": "^1.0.4",
    "node": "^24.9.0",
    "path": "^0.12.7",
    "slugify": "^1.6.6"
  }
}
